


<p id="notice"><%= notice %></p>

<h1 class="test">Listing Trashes</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Catetory</th>
      <th>Trash type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @trashes.each do |trash| %>
      <tr>
        <td><%= trash.title %></td>
        <td><%= trash.description %></td>
        <td><%= trash.catetory_id %></td>
        <td><%= trash.trash_type %></td>
        <td><%= link_to 'Show', trash %></td>
        <td><%= link_to 'Edit', edit_trash_path(trash) %></td>
        <td><%= link_to 'Destroy', trash, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Trash', new_trash_path %>
