


<p id="notice"><%= notice %></p>

<h1 class="test">Listing Trashes</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Catetory</th>
      <th>Trash type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @trash.each do |trash| %>
      <tr>
        <td><%= trash.title %></td>
        <td><%= trash.description %></td>
        <td><%= trash.catetory_id %></td>
        <td><%= trash.trash_type %></td>       
      </tr>
    <% end %>
  </tbody>
</table>

<br>


<%= link_to 'New Trash', new_trash_path %>

